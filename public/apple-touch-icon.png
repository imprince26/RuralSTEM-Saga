<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 180 180" width="180" height="180">
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#10b981;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#059669;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="leafGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#34d399;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#10b981;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="stemGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fbbf24;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f59e0b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background with rounded corners for iOS -->
  <rect x="0" y="0" width="180" height="180" fill="url(#bgGrad)" rx="32" ry="32"/>
  
  <!-- Rural School Building -->
  <g transform="translate(30, 70)">
    <!-- Main Building -->
    <rect x="10" y="30" width="40" height="30" fill="#8b5cf6" rx="2"/>
    <!-- Roof -->
    <polygon points="5,30 30,10 55,30" fill="#6d28d9"/>
    <!-- Door -->
    <rect x="22" y="45" width="8" height="15" fill="#ffffff" rx="1"/>
    <!-- Windows -->
    <rect x="35" y="40" width="8" height="8" fill="#ffffff" rx="1"/>
    <rect x="12" y="40" width="8" height="8" fill="#ffffff" rx="1"/>
    <!-- Door handle -->
    <circle cx="28" cy="52" r="0.8" fill="#059669"/>
  </g>
  
  <!-- STEM Science Atom -->
  <g transform="translate(100, 50)">
    <circle cx="15" cy="15" r="5" fill="none" stroke="url(#stemGrad)" stroke-width="3"/>
    <circle cx="15" cy="15" r="12" fill="none" stroke="url(#stemGrad)" stroke-width="2.5" opacity="0.7"/>
    <circle cx="15" cy="15" r="18" fill="none" stroke="url(#stemGrad)" stroke-width="2" opacity="0.5"/>
    <circle cx="15" cy="15" r="2" fill="url(#stemGrad)"/>
    <!-- Electron orbits -->
    <circle cx="15" cy="5" r="1.5" fill="url(#stemGrad)"/>
    <circle cx="25" cy="15" r="1.5" fill="url(#stemGrad)"/>
    <circle cx="15" cy="25" r="1.5" fill="url(#stemGrad)"/>
  </g>
  
  <!-- Technology Gear -->
  <g transform="translate(40, 110)">
    <circle cx="10" cy="10" r="8" fill="none" stroke="url(#leafGrad)" stroke-width="3"/>
    <circle cx="10" cy="10" r="3" fill="url(#leafGrad)"/>
    <!-- Gear teeth -->
    <rect x="9" y="0" width="2" height="4" fill="url(#leafGrad)"/>
    <rect x="9" y="16" width="2" height="4" fill="url(#leafGrad)"/>
    <rect x="0" y="9" width="4" height="2" fill="url(#leafGrad)"/>
    <rect x="16" y="9" width="4" height="2" fill="url(#leafGrad)"/>
    <rect x="3" y="3" width="3" height="3" fill="url(#leafGrad)" transform="rotate(45 4.5 4.5)"/>
    <rect x="14" y="3" width="3" height="3" fill="url(#leafGrad)" transform="rotate(-45 15.5 4.5)"/>
    <rect x="3" y="14" width="3" height="3" fill="url(#leafGrad)" transform="rotate(-45 4.5 15.5)"/>
    <rect x="14" y="14" width="3" height="3" fill="url(#leafGrad)" transform="rotate(45 15.5 15.5)"/>
  </g>
  
  <!-- Math Pi Symbol -->
  <g transform="translate(115, 110)">
    <text x="0" y="0" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="url(#stemGrad)">Ï€</text>
  </g>
  
  <!-- Growth Leaves -->
  <g transform="translate(130, 80)">
    <path d="M10,20 Q20,5 30,20 Q20,30 10,20 Z" fill="url(#leafGrad)"/>
    <line x1="10" y1="20" x2="20" y2="30" stroke="url(#leafGrad)" stroke-width="2"/>
    <path d="M15,25 Q22,15 29,25 Q22,32 15,25 Z" fill="#34d399" opacity="0.8"/>
  </g>
  
  <!-- Game Controller -->
  <g transform="translate(60, 140)">
    <rect x="0" y="0" width="20" height="12" fill="#8b5cf6" rx="5"/>
    <circle cx="4" cy="6" r="1.5" fill="#ffffff"/>
    <circle cx="16" cy="6" r="1.5" fill="#ffffff"/>
    <rect x="7" y="3" width="6" height="6" fill="#6d28d9" rx="1"/>
  </g>
</svg>